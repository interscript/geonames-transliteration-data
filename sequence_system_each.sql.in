.mode csv
.headers on

SELECT c2.TRANSL_CD, c2.UNI, c2.NT, c2.FULL_NAME_RO, c2.FULL_NAME_RG, c1.UNI, c1.NT, c1.FULL_NAME_RO, c1.FULL_NAME_RG, c2.LC
FROM countries AS c1
LEFT JOIN countries AS c2
ON c2.NAME_LINK = c1.UNI
WHERE c2.TRANSL_CD = "$TRANSLIT_SYSTEM" AND c2.TRANSL_CD <> ""
ORDER BY c2.TRANSL_CD DESC
;
